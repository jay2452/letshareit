digraph models_diagram {
	graph[overlap=false, splines=true, bgcolor="none"]
	"User" [shape=Mrecord, label="{User|id :int(11)\lemail :varchar(255)\lencrypted_password :varchar(255)\lreset_password_token :varchar(255)\lreset_password_sent_at :datetime\lremember_created_at :datetime\lsign_in_count :int(11)\lcurrent_sign_in_at :datetime\llast_sign_in_at :datetime\lcurrent_sign_in_ip :varchar(255)\llast_sign_in_ip :varchar(255)\lcreated_at :datetime\lupdated_at :datetime\lname :varchar(255)\lbranch_id :int(11)\labout_me :text\l}"]
	"Upload" [shape=Mrecord, label="{Upload|id :int(11)\ltopic :varchar(255)\luser_id :int(11)\lsubject_id :int(11)\lcreated_at :datetime\lupdated_at :datetime\lfile_file_name :varchar(255)\lfile_content_type :varchar(255)\lfile_file_size :int(11)\lfile_updated_at :datetime\lapproved :tinyint(1)\l}"]
	"Subject" [shape=Mrecord, label="{Subject|id :int(11)\lname :varchar(255)\lcode :varchar(255)\lcreated_at :datetime\lupdated_at :datetime\lbranch_id :int(11)\lslug :varchar(255)\l}"]
	"Link" [shape=Mrecord, label="{Link|id :int(11)\ltopic :varchar(255)\lurl :varchar(255)\luser_id :int(11)\lsubject_id :int(11)\lcreated_at :datetime\lupdated_at :datetime\lapproved :tinyint(1)\l}"]
	"Branch" [shape=Mrecord, label="{Branch|id :int(11)\lname :varchar(255)\lcreated_at :datetime\lupdated_at :datetime\lslug :varchar(255)\l}"]
	"UserPreference" [shape=Mrecord, label="{UserPreference|id :int(11)\luser_id :int(11)\lsubject_id :int(11)\lcreated_at :datetime\lupdated_at :datetime\l}"]
	"Role" [shape=Mrecord, label="{Role|id :int(11)\lname :varchar(255)\lresource_id :int(11)\lresource_type :varchar(255)\lcreated_at :datetime\lupdated_at :datetime\l}"]
	"Feedback" [shape=Mrecord, label="{Feedback|id :int(11)\luser_name :varchar(255)\lemail :varchar(255)\lcontent :text\lcreated_at :datetime\lupdated_at :datetime\l}"]
	"User" -> "Role" [arrowtail=crow, arrowhead=crow, dir=both color="#592CFE"]
	"User" -> "Upload" [arrowtail=odot, arrowhead=crow, dir=both color="#1282C5"]
	"User" -> "UserPreference" [arrowtail=odot, arrowhead=crow, dir=both color="#87E9CE"]
	"User" -> "Subject" [arrowtail=crow, arrowhead=crow, dir=both color="#105BD9"]
	"User" -> "Link" [arrowtail=odot, arrowhead=crow, dir=both color="#B1FA3D"]
	"Subject" -> "Upload" [arrowtail=odot, arrowhead=crow, dir=both color="#52A08D"]
	"Subject" -> "Link" [arrowtail=odot, arrowhead=crow, dir=both color="#CB8291"]
	"Subject" -> "UserPreference" [arrowtail=odot, arrowhead=crow, dir=both color="#4AC917"]
	"Branch" -> "User" [arrowtail=odot, arrowhead=crow, dir=both color="#A3DC36"]
	"Branch" -> "Subject" [arrowtail=odot, arrowhead=crow, dir=both color="#1C28A4"]
}
